{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 80,
    "overrides": [
      {
        "files": "*.html",
        "options": { "parser": "html" }
      },
      {
        "files": "*.css",
        "options": { "parser": "css" }
      },
      {
        "files": "*.ts",
        "options": { "parser": "typescript" }
      }
    ]
  }